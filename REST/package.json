{
  "name": "invader-detector",
  "version": "1.0.0",
  "description": "Service used to manage resources from Invader Detector project.",
  "main": "server.js",
  "scripts": {
    "build": "gulp build",
    "build:doc": "typedoc --options typedoc.json",
    "start": "node dist/server.js",
    "start:dev": "gulp dev",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "nyc --clean --reporter=text-summary mocha --opts test/mocha.opts test/**/*.spec.ts",
    "test:unit": "nyc --clean --all --reporter=text-summary mocha --opts test/mocha.opts test/unit/**/*.spec.ts",
    "test:integration": "nyc --clean --all --reporter=text-summary mocha --opts test/mocha.opts test/integration/**/*.spec.ts",
    "test:single": "mocha --opts test/mocha.opts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lucasrochagit/invader-detector.git"
  },
  "homepage": "",
  "keywords": [
    "api",
    "api-template",
    "nodejs"
  ],
  "author": "Lucas Rocha <lucas.rocha@nutes.uepb.edu.br>",
  "license": "ISC",
  "nyc": {
    "include": [
      "**/*.tsx",
      "**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/*",
      "config/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "aws-sdk": "^2.739.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "query-strings-parser": "^2.1.3",
    "randomstring": "^1.1.5",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.3",
    "@types/yamljs": "^0.2.30",
    "dotenv": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "bugs": {}
}
